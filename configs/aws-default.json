{
  "infrastructure": {
    "region": "us-east-1",
    "instance_type": "t3.medium",
    "ami": "ami-0c55b159cbfafe1f0",
    "admin_username": "ubuntu",
    "ssh_public_key_path": "~/.ssh/id_rsa.pub",
    "os_disk_size_gb": 30,
    "tags": {
      "Application": "Coolify",
      "ManagedBy": "cool-kit"
    }
  },
  "networking": {
    "vpc_cidr": "10.0.0.0/16",
    "subnet_cidr": "10.0.1.0/24",
    "ssh_port": 22,
    "app_port": 80,
    "app_ssl_port": 443,
    "coolify_port": 8000,
    "websocket_port": 6001
  },
  "coolify": {
    "default_admin_email": "admin@coolify.local",
    "app_url_template": "http://{public_ip}",
    "pusher_host_template": "{public_ip}",
    "pusher_port": 6001
  },
  "paths": {
    "remote_base": "/home/ubuntu/coolify",
    "remote_env": "/home/ubuntu/coolify/.env",
    "remote_backups": "/home/ubuntu/coolify/backups"
  },
  "docker": {
    "registry_url": "ghcr.io",
    "helper_image": "coollabsio/coolify-helper",
    "app_image": "coollabsio/coolify",
    "realtime_image": "coollabsio/coolify-realtime"
  }
}
